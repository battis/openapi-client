{
  "name": "battis/openapi-client-generator",
  "description": "Generate a PHP client for a RESTful API from its OpenAPI description file",
  "repositories": [
    {
      "type": "path",
      "url": "../php-generator"
    }
  ],
  "require": {
    "battis/data-utilities": "^1.2",
    "battis/loggable": "dev-main",
    "battis/php-generator": "dev-main",
    "cebe/php-openapi": "^1.7",
    "friendsofphp/php-cs-fixer": "^3.49",
    "league/html-to-markdown": "^5.1",
    "league/oauth2-client": "^2.7",
    "psr/log": "^3.0",
    "psr/simple-cache": "^3.0",
    "splitbrain/php-cli": "^1.3",
    "sspat/reserved-words": "^3.0",
    "symfony/filesystem": "^7.0"
  },
  "license": "GPL-3.0",
  "authors": [
    {
      "name": "Seth Battis",
      "email": "seth@battis.net"
    }
  ],
  "autoload": {
    "psr-4": {
      "Battis\\OpenAPI\\": "src"
    }
  },
  "config": {
    "sort-packages": true
  }
}
